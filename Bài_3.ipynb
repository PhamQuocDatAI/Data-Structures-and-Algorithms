    "# Tính giai thừa của một số\n",
    "def giai_thua(n):\n",
    "    while n < 0:\n",
    "        print(\"Lỗi giá trị n.\")\n",
    "        n = int(input(\"Nhập giá trị n: \"))\n",
    "    if n == 0:\n",
    "        return 1\n",
    "    return n * giai_thua(n-1)\n",
    "\n",
    "n = int(input(\"Nhập giá trị n: \"))\n",
    "print(giai_thua(n))"
   ]
  }

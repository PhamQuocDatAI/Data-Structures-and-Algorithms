   "source": [
    "# Ước số chung lớn nhất\n",
    "def UCLN(a, b):\n",
    "    if (b == 0):\n",
    "        return a;\n",
    "    return UCLN(b, a % b);\n",
    " \n",
    "a = int(input(\"Nhập số nguyên dương a = \"));\n",
    "b = int(input(\"Nhập số nguyên dương b = \"));\n",
    "#tính USCLN của a và b\n",
    "print(\"Ước chung lớn nhất của\", a, \"và\", b, \"là:\", UCLN(a, b))"
   ]
  }
